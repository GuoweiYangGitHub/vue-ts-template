{
  "name": "vue-ts-template",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "build:stage": "vue-tsc --noEmit && vite build --mode=staging",
    "build:prod": "vue-tsc --noEmit && vite build --mode=production",
    "preview": "vite preview",
    "prepare": "husky install",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prettier": "prettier . --write"
  },
  "lint-staged": {
    "src/**/*.{html,vue,css,scss}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "src/**/*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.18.5",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "element-plus": "^2.2.6",
    "pinia": "^2.0.14",
    "sass": "^1.53.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@iconify-json/ep": "^1.1.6",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "lint-staged": "12.4.1",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.1",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^2.9.9",
    "vite-plugin-inspect": "^0.5.1",
    "vue-tsc": "^0.34.7"
  }
}
